<h2>Solutions</h2>

{{#each solutions}}
<li>{{this.solution}}
<button type="button"  class="btn btn-primary edit-solution-btn" data-id="{{this.id}}" data-toggle="modal" data-target="#edit-symptom-modal">Update</button>
<button type="button"  class="btn btn-danger delete-solution-btn" data-id="{{this.id}}" data-toggle="modal" data-target="#delete-rescue-modal">Delete</button>
</li>
{{/each}}
